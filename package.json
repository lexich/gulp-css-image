{
  "name": "gulp-css-image",
  "version": "0.3.0",
  "description": "A plugin for Gulp",
  "keywords": [
    "gulpplugin"
  ],
  "author": {
    "name": "Efremov Alex",
    "email": "lexich121@gmail.com",
    "url": "https://github.com/lexich"
  },
  "repository": "lexich/gulp-css-image",
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "release": "npm test && standard-version && git push --follow-tags origin master --no-verify && npm publish"
  },
  "dependencies": {
    "css-image": "0.2.3",
    "gulp-util": "3.0.7",
    "image-size": "^0.6.2",
    "lodash": "4.13.1",
    "through2": "2.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "event-stream": "3.3.3",
    "gulp": "3.9.1",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.2.0",
    "standard-version": "^4.3.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
